{
    "root": "apps/hr-updates",
    "sourceRoot": "apps/hr-updates/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/web:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "babel",
                "outputPath": "dist/apps/hr-updates",
                "index": "apps/hr-updates/src/index.html",
                "baseHref": "/",
                "main": "apps/hr-updates/src/main.tsx",
                "polyfills": "apps/hr-updates/src/polyfills.ts",
                "tsConfig": "apps/hr-updates/tsconfig.app.json",
                "scripts": [],
                "webpackConfig": "webpack.config.js"
            },
            "configurations": {
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "apps/hr-updates/src/environments/environment.ts",
                            "with": "apps/hr-updates/src/environments/environment.prod.ts"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                }
            }
        },
        "serve": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "buildTarget": "hr-updates:build:development",
                "hmr": true
            },
            "configurations": {
                "production": {
                    "buildTarget": "hr-updates:build:production",
                    "hmr": false
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/hr-updates/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/hr-updates"],
            "options": {
                "jestConfig": "apps/hr-updates/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
