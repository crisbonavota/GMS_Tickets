{
    "root": "libs/api-utils",
    "sourceRoot": "libs/api-utils/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/api-utils/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/api-utils"],
            "options": {
                "jestConfig": "libs/api-utils/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "configurations": {
        "production": {
            "fileReplacements": [
                {
                    "replace": "libs/api-utils/src/environments/environment.ts",
                    "with": "libs/api-utils/src/environments/environment.prod.ts"
                }
            ]
        }
    }
}
